    <object id="72_30863873" type="text" x="20994.814143346634" y="1493.2024815237464" r="-2.7759820227195356e-31" s="1.612039768649427" class="body">
      <height>783.9359999999998</height>
      <width>864.223778482768</width>
      <p>
        <text><![CDATA[Install PHP]]></text>
      </p>
      <p align="center">
        <text style="italic"><![CDATA[sudo yum install php]]></text>
        <text><![CDATA[ ]]></text>
      </p>
      <p>
        <text><![CDATA[]]></text>
      </p>
      <p>
        <text><![CDATA[Restart httpd server]]></text>
      </p>
      <p align="center">
        <text style="italic"><![CDATA[service httpd restart]]></text>
      </p>
      <p>
        <text><![CDATA[]]></text>
      </p>
      <p>
        <text><![CDATA[Test]]></text>
      </p>
      <p>
        <text><![CDATA[Test PHPGo to /var/www/html/ directory: ]]></text>
      </p>
      <p align="center">
        <text style="italic"><![CDATA[# cd /var/www/html/]]></text>
      </p>
      <p align="center">
        <text style="italic"><![CDATA[]]></text>
      </p>
      <p>
        <text><![CDATA[Create a php file: ]]></text>
      </p>
      <p align="center">
        <text style="italic"><![CDATA[#vi phptest.php]]></text>
      </p>
      <p align="center">
        <text><![CDATA[]]></text>
      </p>
      <p>
        <text><![CDATA[Append following lines:]]></text>
      </p>
      <p>
        <text><![CDATA[]]></text>
      </p>
      <p>
        <text><![CDATA[]]></text>
      </p>
      <p>
        <text><![CDATA[ ]]></text>
      </p>
      <p>
        <text><![CDATA[Save and close the file.]]></text>
      </p>
      <p>
        <text><![CDATA[]]></text>
      </p>
      <p>
        <text><![CDATA[Open web browser and type the url:]]></text>
      </p>
      <p align="center">
        <text style="italic"><![CDATA[ http://localhost/phptest.php ]]></text>
      </p>
      <p align="center">
        <text style="italic"><![CDATA[or]]></text>
      </p>
      <p align="center">
        <text style="italic"><![CDATA[ http://server-ip-address/phptest.php.]]></text>
      </p>
      <p>
        <text><![CDATA[]]></text>
      </p>
